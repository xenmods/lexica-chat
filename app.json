{
  "expo": {
    "name": "Lexica",
    "slug": "test-app",
    "version": "1.0.0",
    "scheme": "lexica",
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/robot.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.xenmods.lexica"
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "dfe14ac1-371c-40d2-a768-0558c72881e3"
      }
    }
  }
}
